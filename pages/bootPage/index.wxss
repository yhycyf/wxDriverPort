/**index.wxss**/
.bootPage {
  width: 100%;
  box-sizing: border-box;
  min-height: 100vh;
  padding-bottom: 200rpx;
  background: #EEF0F3;
}
.bootPage .header {
  width: 100%;
  height: 370rpx;
  background: url('https://img.yzcdn.cn/vant/cat.jpeg') no-repeat;
  background-size: cover;
  position: relative;
}
.bootPage .header .title {
  position: absolute;
  left: 32rpx;
  bottom: 70rpx;
  font-size: 48rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #FFFFFF;
}
.bootPage .selectCity {
  width: 100%;
  height: 104rpx;
  background: #ffffff;
  justify-content: space-between;
  padding: 0 30rpx;
  box-sizing: border-box;
}
.bootPage .selectCity .left {
  font-size: 28rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #151515;
}
.bootPage .selectCity .right .text {
  margin-left: 6rpx;
  margin-right: 8rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #457CFB;
}
.bootPage .orderList {
  width: 100%;
  padding: 38rpx 0 52rpx;
  box-sizing: border-box;
  margin-top: 20rpx;
  background: #ffffff;
}
.bootPage .orderList .titleBox .iconBox {
  padding: 0 30rpx;
  box-sizing: border-box;
  height: 18rpx;
  overflow: hidden;
}
.bootPage .orderList .titleBox .iconBox view {
  width: 12rpx;
  height: 30rpx;
}
.bootPage .orderList .titleBox .iconLeftBox view,
.bootPage .orderList .titleBox .iconRightBox view {
  transform: rotate(-20deg);
}
.bootPage .orderList .titleBox .iconLeftBox .icon1,
.bootPage .orderList .titleBox .iconRightBox .icon1 {
  background: #CEDEFF;
}
.bootPage .orderList .titleBox .iconLeftBox .icon2,
.bootPage .orderList .titleBox .iconRightBox .icon2 {
  background: #95B4FF;
}
.bootPage .orderList .titleBox .iconLeftBox .icon3,
.bootPage .orderList .titleBox .iconRightBox .icon3 {
  background: #457BFB;
}
.bootPage .orderList .titleBox .iconLeftBox .icon2,
.bootPage .orderList .titleBox .iconRightBox .icon2 {
  margin: 0 12rpx;
}
.bootPage .orderList .titleBox .iconRightBox {
  transform: rotate(180deg);
}
.bootPage .orderList .titleBox .iconRightBox view {
  transform: rotate(20deg);
}
.bootPage .orderList .listBox {
  width: 100%;
  padding: 0 100rpx;
  box-sizing: border-box;
  justify-content: space-between;
  margin-top: 42rpx;
}
.bootPage .orderList .listBox .list image {
  width: 98rpx;
  height: 98rpx;
  border-radius: 50%;
}
.bootPage .orderList .listBox .list .text {
  margin-top: 8rpx;
  font-size: 28rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #151515;
}
.bootPage .signInKnow {
  padding: 28rpx 0 32rpx 32rpx;
  box-sizing: border-box;
}
.bootPage .signInKnow .title {
  font-size: 28rpx;
  font-family: PingFangSC-Semibold, PingFang SC;
  font-weight: 600;
  color: #151515;
}
.bootPage .signInKnow .require {
  margin-top: 24rpx;
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #777777;
}
.bootPage .signInKnow .textContent {
  width: 100%;
  align-items: flex-start;
}
.bootPage .signInKnow .textContent .p {
  font-size: 26rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #787878;
  margin-top: 14rpx;
}
.bootPage .fixedBottom {
  width: 100%;
  background: #FFFFFF;
  padding: 28rpx 30rpx 16rpx;
  box-sizing: border-box;
  position: fixed;
  left: 0;
  bottom: 0;
}
.bootPage .fixedBottom .agree {
  width: 100%;
  justify-content: flex-start;
}
.bootPage .fixedBottom .labelClass {
  font-size: 24rpx;
  font-family: PingFangSC-Regular, PingFang SC;
  font-weight: 400;
  color: #777777;
  margin-left: 6rpx;
}
.bootPage .fixedBottom .signinLoginBox {
  width: 100%;
  margin-top: 24rpx;
}
.bootPage .fixedBottom .signinLoginBox .btn {
  width: 332rpx;
  height: 80rpx;
  border-radius: 8rpx;
}
.bootPage .fixedBottom .signinLoginBox .left {
  font-size: 32rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #151515;
}
.bootPage .fixedBottom .signinLoginBox .right {
  background: #457CFB;
  font-size: 32rpx;
  font-family: PingFangSC-Medium, PingFang SC;
  font-weight: 500;
  color: #FFFFFF;
}
